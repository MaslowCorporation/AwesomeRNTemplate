{
  "name": "arduinogpt",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "reset-main-branch": "git checkout --orphan latest_branch && git add -A && git commit -am \"First commit ;-)\" && git branch -D main && git branch -m main && git push -f --set-upstream origin main",
    "first-push": "npm run git-init && git add . && git commit -m \"first commit\" && git branch -M main && git remote add origin <GITHUB_URL> && git push -u origin main",
    "git-init": "rimraf .git && git init",
    "push": "(git add . && git commit -m \"I'm a bigger pusher than Ice-T homie !!!\" && git push) || exit 0",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint .",
    "fix-lint-errors": "eslint . --fix",
    "eas-init": "npx eas login && npx eas build:configure",
    "eas-debug-build-android": "npx eas build -p android --profile development",
    "adb-connect": "adb tcpip 57391 && adb connect 100.108.106.94:57391",
    "adb-disconnect": "(adb disconnect) || exit 0",
    "start-debug-android": "(npm run adb-connect && npm run android) || exit 0",
    "start-debug-ios": "(npm run adb-connect && npm run ios) || exit 0",
    "clean-app": "cd android && gradlew clean && cd ..",
    "clean-android-resources": "rimraf android/app/src/main/res/raw android/app/src/main/res/drawable-hdpi android/app/src/main/res/drawable-mdpi android/app/src/main/res/drawable-xhdpi android/app/src/main/res/drawable-xxhdpi android/app/src/main/res/drawable-xxxhdpi",
    "del-sha-keys": "keytool -delete -v -alias androiddebugkey -keystore android/app/debug.keystore -storepass android",
    "gen-sha-keys": "keytool -genkey -v -keystore android/app/debug.keystore -alias androiddebugkey -storepass android -keypass android -keyalg RSA -validity 14000",
    "get-sha-keys": "keytool -list -v -keystore android/app/debug.keystore -alias androiddebugkey -storepass android",
    "del-sha-keys-release": "keytool -delete -v -alias androidreleasekey -keystore android/app/release.keystore -storepass android",
    "gen-sha-keys-release": "keytool -genkey -v -keystore android/app/release.keystore -alias androidreleasekey -storepass android -keypass android -keyalg RSA -validity 14000",
    "get-sha-keys-release": "keytool -list -v -keystore android/app/release.keystore -alias androidreleasekey -storepass android",
    "copy-apk-release-windows": "copy android\\app\\build\\outputs\\apk\\release\\app-release.apk %USERPROFILE%\\Desktop",
    "copy-apk-release-linux": "cp ./android/app/build/outputs/apk/release/app-release.apk ~/Desktop",
    "copy-apk-release-macos": "cp ./android/app/build/outputs/apk/release/app-release.apk ~/Desktop",
    "copy-apk-windows": "copy android\\app\\build\\outputs\\apk\\debug\\app-debug.apk %USERPROFILE%\\Desktop",
    "copy-apk-linux": "cp ./android/app/build/outputs/apk/debug/app-debug.apk ~/Desktop",
    "copy-apk-macos": "cp ./android/app/build/outputs/apk/debug/app-debug.apk ~/Desktop",
    "copy-aab-release-windows": "copy android\\app\\build\\outputs\\bundle\\release\\app-release.aab %USERPROFILE%\\Desktop",
    "copy-aab-release-linux": "cp ./android/app/build/outputs/bundle/release/app-release.aab ~/Desktop",
    "copy-aab-release-macos": "cp ./android/app/build/outputs/bundle/release/app-release.aab ~/Desktop",
    "build-android-debug": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && gradlew assembleDebug && cd ..",
    "build-android-release": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && npm run clean-android-resources && cd android && gradlew assembleRelease && cd ..",
    "build-android-release-aab": "cd android && gradlew bundleRelease && cd ..",
    "build-android-debug-quick": "cd android && gradlew assembleDebug && cd ..",
    "build-android-release-quick": "cd android && gradlew assembleRelease && cd ..",
    "shake-device": "adb shell input keyevent 82"
  },
  "dependencies": {
    "@notifee/react-native": "^7.8.2",
